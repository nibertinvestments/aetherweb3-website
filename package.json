{
  "name": "aetherweb3-website",
  "version": "1.0.0",
  "description": "Production-ready multi-blockchain gateway service providing reliable RPC access to Ethereum, Base, and Arbitrum networks",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest",
    "security-audit": "npm audit --audit-level=moderate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "echo 'Building for production...'",
    "setup": "node setup-environment.js",
    "postinstall": "npm run security-audit"
  },
  "keywords": [
    "ethereum",
    "base", 
    "arbitrum",
    "blockchain",
    "rpc",
    "api-gateway",
    "multi-chain",
    "web3",
    "evm",
    "defi",
    "enterprise",
    "api-management",
    "cryptocurrency",
    "smart-contracts"
  ],
  "author": {
    "name": "Nibert Investments LLC",
    "email": "support@nibertinvestments.com",
    "url": "https://nibertinvestments.com"
  },
  "contributors": [
    {
      "name": "AetherWeb3 Development Team",
      "email": "dev@nibertinvestments.com"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.8.1",
    "express-slow-down": "^1.6.0",
    "http-proxy-middleware": "^2.0.6",
    "ws": "^8.14.2",
    "stripe": "^12.18.0",
    "@google-cloud/firestore": "^6.8.0",
    "@google-cloud/kms": "^3.2.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.6.7",
    "crypto": "^1.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "eslint": "^8.47.0",
    "supertest": "^6.3.3",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nibertinvestments/aetherweb3-website.git"
  },
  "bugs": {
    "url": "https://github.com/nibertinvestments/aetherweb3-website/issues",
    "email": "support@nibertinvestments.com"
  },
  "homepage": "https://nibertinvestments.github.io/aetherweb3-website/",
  "funding": {
    "type": "individual",
    "url": "https://nibertinvestments.com"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    }
  }
}